@model IEnumerable<AspNetBookstore.Models.Category>
@{
    ViewBag.Title = "Store";
}

<h3>Browse Genres</h3>

<p>
    Select from @Model.Count() genres:
</p>
<ul>
    @foreach (var category in Model)
    {
        <li>@Html.ActionLink(category.Name, "Browse", new { category = category.Name })</li>
    }
</ul>